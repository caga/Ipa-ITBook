<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <servlet>
    <servlet-name>addscheduleentry</servlet-name>
    <servlet-class>com.nealford.art.ixbeans.servlet.ui.AddScheduleEntry</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>servlet1</servlet-name>
    <servlet-class>com.nealford.art.ixbeans.servlet.ui.Servlet1</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>viewschedule</servlet-name>
    <servlet-class>com.nealford.art.ixbeans.servlet.ui.ViewSchedule</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addscheduleentry</servlet-name>
    <url-pattern>/addscheduleentry</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>servlet1</servlet-name>
    <url-pattern>/servlet1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>viewschedule</servlet-name>
    <url-pattern>/viewschedule</url-pattern>
  </servlet-mapping>
  <taglib>
    <taglib-uri>/internetbeans.tld</taglib-uri>
    <taglib-location>/WEB-INF/lib/internetbeans.jar</taglib-location>
  </taglib>
</web-app>
